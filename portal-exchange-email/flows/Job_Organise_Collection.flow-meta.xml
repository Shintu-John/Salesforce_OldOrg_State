        <dataType>String</dataType>
        <expression>IF(TEXT({!jobObj.Type__c})=&quot;Standard&quot;,&quot;Fixed_Job_Template&quot;,
IF(TEXT({!jobObj.Type__c})=&quot;Wait &amp; Load&quot;,&quot;Variable_Job_Template&quot;,
IF(TEXT({!jobObj.Type__c})=&quot;Rebate&quot;,&quot;Rebate_Job_Template&quot;,&quot;No Template Found&quot;)))</expression>
    </formulas>
